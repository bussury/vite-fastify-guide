{
  "name": "vite-tailwind-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node  server.mjs",
    "watch": "nodemon  server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry/entry-server.js --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node   server.mjs"
  },
  "dependencies": {
    "@fastify/compress": "^5.0.0",
    "@vueuse/head": "^0.7.6",
    "fastify": "^3.29.0",
    "fastify-static": "^4.7.0",
    "middie": "^6.1.0",
    "serve-static": "^1.15.0",
    "vue": "^3.2.34",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.34",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.9"
  },
  "resolutions": {
    "postcss": "^8.4.14"
  }
}
