{
  "name": "vite-tailwind-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node  server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr entry/entry-server.js --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node  --experimental-json-modules server.mjs"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.5",
    "fastify": "^3.27.4",
    "fastify-compress": "^4.0.1",
    "fastify-static": "^4.6.1",
    "middie": "^6.0.0",
    "serve-static": "^1.15.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "vite": "^2.9.1"
  },
  "resolutions": {
    "postcss": "^8.4.12"
  }
}
