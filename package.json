{
  "name": "vite-tailwind-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node  server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr entry/entry-server.js --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node  --experimental-json-modules server.mjs"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.4",
    "fastify": "^3.25.3",
    "fastify-compress": "^4.0.1",
    "fastify-static": "^4.5.0",
    "middie": "^5.4.0",
    "serve-static": "^1.14.2",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "autoprefixer": "^10.4.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.8",
    "vite": "^2.7.10"
  },
  "resolutions": {
    "postcss": "^8.4.5"
  }
}
